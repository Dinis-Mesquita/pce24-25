                    <items id="text">Not current user</items>
                    <items id="description">Individual has previously used contraception but is not a current user.</items>
                </term_definitions>
                <term_definitions code="at0006">
                    <items id="text">Never used</items>
                    <items id="description">Individual has never used any type of contraception.</items>
                </term_definitions>
                <term_definitions code="at0013">
                    <items id="text">Episode start date</items>
                    <items id="description">Date when this episode commenced.</items>
                    <items id="comment">Can be a partial date, for example, only a year.</items>
                </term_definitions>
                <term_definitions code="at0019">
                    <items id="text">Overall comment</items>
